    "class Solution:\n",
    "    def reverseBits(self, n: int) -> int:\n",
    "        \n",
    "        res = 0\n",
    "        for i in range(31):\n",
    "            res = res | (n & 1)\n",
    "            res = res << 1\n",
    "            n = n >> 1\n",
    "        \n",
    "        res = res | (n&1)    \n",
    "        return res"
   ]
  }
