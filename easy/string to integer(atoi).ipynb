   "source": [
    "import re\n",
    "\n",
    "class Solution:\n",
    "    def myAtoi(self, str: str) -> int:\n",
    "        isMatched = re.search('^[\\+\\-]?\\d+',str.lstrip())\n",
    "        if isMatched:\n",
    "            return min(max(int(isMatched.group()),-2**31),2**31 - 1)\n",
    "        else:\n",
    "            return 0"
   ]
